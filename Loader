Status_Text.BackgroundTransparency = 1.000
Status_Text.Position = UDim2.new(0, 0, 0.519672155, 0)
Status_Text.Size = UDim2.new(1, 0, 0.0688524619, 0)
Status_Text.Font = Enum.Font.SourceSansBold
